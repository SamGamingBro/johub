		if script.Parent.Parent.Parent.LoadStart.Visible == true then
			script.Parent.Parent.Parent.LoadStart.Load1.Visible = true
			wait(4)
			script.Parent.Parent.Parent.LoadStart.Load3.Visible = true
			wait(4)
			script.Parent.Parent.Parent.LoadStart.Load2.Visible = true
			wait(3)
			script.Parent.Parent.Parent.LoadStart.Visible = false
			script.Parent.Parent.Parent.Failed.Visible = true 
			wait(2)
			script.Parent.Parent.Parent.Failed.Visible = false
			script.Parent.Parent.Parent.LoadStart.Load1.Visible = false
			script.Parent.Parent.Parent.LoadStart.Load2.Visible = false
			script.Parent.Parent.Parent.LoadStart.Load3.Visible = false
		else
			if script.Parent.Parent.Blinker.Active then
				script.Parent.Parent.Parent.Failed.Text "Everything failed please restart!!!!!!"
			end
		end
	end)
end
